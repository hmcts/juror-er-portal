{
  "useCSRFProtection": true,
  "appInsights": {
    "instrumentationKey": false
  },
  "security": {
    "referrerPolicy": "same-origin",
    "hsts": {
      "maxAge": 31536000,
      "includeSubDomains": true,
      "preload": true
    }
  },
  "secrets": {
    "juror": {
      "er-portal-sessionSecret": "[super-secret-key][super-secret-key][super-secret-key]",
      "app-insights-connection-string": "very-secure-key"
    }
  }
}
